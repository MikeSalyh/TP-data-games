<s:TitleWindow xmlns:fx="http://ns.adobe.com/mxml/2009" 
			   xmlns:s="library://ns.adobe.com/flex/spark" 
			   xmlns:mx="library://ns.adobe.com/flex/mx" width="260"  title="      Choose Game Level" 
			   fontSize="16" textAlign="center" backgroundColor="#7B9453" enabled="true" 
			   skinClass="spark.skins.spark.TitleWindowSkin">
	
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<s:layout>
		<s:VerticalLayout paddingLeft="12" paddingTop="12" paddingRight="12" paddingBottom="12" gap="12" rowHeight="40" variableRowHeight="false" horizontalAlign="contentJustify"/>
